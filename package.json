    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "npm": "^5.5.1",
    "npm-run-all": "^4.1.1",
    "open": "0.0.5",
    "react-hot-loader": "^1.3.1",
    "react-router-test-context": "^0.1.0",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.15.0"
